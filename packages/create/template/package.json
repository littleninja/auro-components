{
  "name": "@littleninja/[namespace]-[name]",
  "version": "1.0.0",
  "description": "> TODO: description",
  "repository": {
    "type": "git",
    "url": "https://github.com/littleninja/auro-components.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/littleninja"
  },
  "homepage": "",
  "license": "ISC",
  "type": "module",
  "main": "dist/[namespace]-[name].js",
  "directories": {
    "bin": "dist",
    "src": "src",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-s clean build:js",
    "build:watch": "run-s clean build:js-watch",
    "build:js": "rollup --input src/[namespace]-[name].js --file dist/[namespace]-[name].js --config ../../rollup.config.js ",
    "build:js-watch": "rollup --input src/[namespace]-[name].js --file dist/[namespace]-[name].js --config ../../rollup.config.js --watch",
    "clean": "rimraf dist",
    "doc:custom-elements": "wca analyze src --format json --outFile custom-elements.json",
    "doc:readme": "wca analyze src --format markdown --outFile readme.md"
  },
  "peerDependencies": {
    "@alaskaairux/design-tokens": "^3.0.0",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "focus-visible": "^5.2.0"
  },
  "dependencies": {
    "lit-element": "^2.3.1",
    "lit-html": "^1.2.1"
  },
  "devDependencies": {
    "@alaskaairux/design-tokens": "^3.0.0",
    "@webcomponents/webcomponentsjs": "^2.5.0",
    "focus-visible": "^5.2.0"
  }
}
