{
  "name": "auro-thing",
  "version": "1.0.0",
  "description": "> TODO: description",
  "homepage": "",
  "license": "ISC",
  "type": "module",
  "main": "dist/auro-thing.js",
  "directories": {
    "bin": "dist",
    "src": "src",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "run-s clean build:js",
    "build:watch": "run-s clean build:js-watch",
    "build:js": "rollup -i src/auro-thing.js -o dist/auro-thing.js -c ../../rollup.config.js ",
    "build:js-watch": "rollup -i src/auro-thing.js -o dist/auro-thing.js -c ../../rollup.config.js --watch.include src/** ",
    "clean": "rimraf dist",
    "doc:custom-elements": "wca analyze src --format json --outFile custom-elements.json",
    "doc:readme": "wca analyze src --format markdown --outFile readme.md"
  },
  "dependencies": {
    "lit-element": "^2.3.1",
    "lit-html": "^1.2.1"
  }
}
